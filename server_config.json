{
    "mcpServers": {
        "filesystem": {
            "command": "sh",
            "args": [
                "-c",
                "npx -y @modelcontextprotocol/server-filesystem . 2>/dev/null"
            ]
        },
        "research": {
            "command": "sh",
            "args": [
                "-c",
                "uv run research_server.py 2>/dev/null"
            ]
        },
        "fetch": {
            "command": "sh",
            "args": [
                "-c",
                "uvx mcp-server-fetch 2>/dev/null"
            ]
        }
    }
}
  